.topfix
  display flex
  position fixed
  z-index 100
  justify-content space-between
  align-items center
  padding 0 10px
  left 0
  right 0
  height 84px
  background-color #d43c33
  .topfix-left
    flex 1
    font-size 25px
    color #fff
  .topfix-right
    font-size 16px
    color #d43c33
    background-color #fff
    height 36px
    line-height 36px
    width 100px
    text-align center
    border-radius 37.5%/100%